{
    "name": "air-service-build",
    "source": {
        "type": "GITHUB",
        "location": "https://github.com/cc4i/go-bumblebee.git",
        "gitCloneDepth": 0,
        "gitSubmodulesConfig": {
            "fetchSubmodules": true
        },
        "buildspec": "src/air/buildspec.yaml",
        "reportBuildStatus": false,
        "insecureSsl": false
    },
    "sourceVersion": "new-combo-jazz",
    "artifacts": {
        "type": "NO_ARTIFACTS"
    },
    "cache": {
        "type": "NO_CACHE"
    },
    "environment": {
        "type": "LINUX_CONTAINER",
        "image": "aws/codebuild/standard:2.0",
        "computeType": "BUILD_GENERAL1_SMALL",
        "privilegedMode": true,
        "imagePullCredentialsType": "CODEBUILD"
    },
    "timeoutInMinutes": 60,
    "queuedTimeoutInMinutes": 480,
    "badgeEnabled": true,
    "logsConfig": {
        "cloudWatchLogs": {
            "status": "ENABLED"
        }
    }
}
